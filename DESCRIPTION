Package: ggmapinset
Type: Package
Title: Add Inset Panels to Maps
Version: 0.4.0.9000
Authors@R:
    c(person(given = "Carl",
             family = "Suster",
             email = "carl.suster@sydney.edu.au",
             role = c("aut", "cre"),
             comment = c(ORCID = "0000-0001-7021-9380")),
      person("Western Sydney Local Health District, NSW Health",
             role = "cph",
